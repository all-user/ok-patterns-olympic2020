{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      1,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": 1,
    "no-unused-vars": 1,
    "no-duplicate-imports": 0,
    "import/no-duplicates": 2,
    "flowtype/define-flow-type": 1,
    "flowtype/use-flow-type": 1
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "mocha": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "plugins": [
    "flowtype",
    "import"
  ]
}
