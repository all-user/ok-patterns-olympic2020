{
  "name": "@all-user/ok-patterns-olympic2020",
  "version": "0.1.6",
  "description": "Patterns Plugin for OK BLOCKS.",
  "main": "lib/index.js",
  "scripts": {
    "help": "npm-run-help",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "run-p lint:*",
    "lint:src": "eslint src/**/*.js",
    "lint:test": "eslint test/**/*.test.js",
    "build:js": "babel src -x .js -d lib",
    "build:css": "stylus -u nib -c stylus/index.styl -o dist/bundle.css -m",
    "build:browser": "browserify -t babelify -r ./src/index.js:@all-user/ok-patterns-olympic2020 | uglifyjs > ./dist/bundle.js",
    "build:copy": "run-p build:copy:*",
    "build:copy:flow": "cpx \"src/*.js.flow\" lib",
    "watch:js": "babel -w src -x .js -d lib",
    "watch:css": "stylus -u nib -c -w stylus/index.styl -o dist/bundle.css -m",
    "watch:copy": "run-p watch:copy:*",
    "watch:copy:flow": "cpx -w \"src/*.js.flow\" lib",
    "clean": "run-s clean:*",
    "clean:lib": "rimraf lib && mkdirp lib",
    "clean:browser": "rimraf dist && mkdirp dist",
    "release": "run-s clean build",
    "build": "run-s build:*",
    "watch": "run-p watch:*",
    "start": "npm run watch"
  },
  "author": "Keita Okamoto <ok@aaa.email.ne.jp>",
  "license": "MIT",
  "devDependencies": {
    "@all-user/ok-blocks": "^0.1.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "cpx": "^1.5.0",
    "eslint": "^3.19.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "exorcist": "^0.4.0",
    "flow-bin": "^0.47.0",
    "flow-typed": "^2.1.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.3.0",
    "nib": "^1.1.2",
    "npm-run-all": "^4.0.2",
    "npm-run-help": "^1.1.0",
    "power-assert": "^1.4.2",
    "rimraf": "^2.6.1",
    "stylus": "^0.54.5",
    "uglify-js": "^2.8.22",
    "watchify": "^3.9.0"
  },
  "dependencies": {}
}
